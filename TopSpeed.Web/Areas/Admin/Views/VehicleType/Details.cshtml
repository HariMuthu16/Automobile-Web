@model VehicleType

<div class="container">
  <div class="card">

      <div class=" card-header text-uppercase text-center text-primary">
            VehicleType Deatils
       </div>

       <div class="card-body">
           <div class="row">
               <div class="col-md-8">

                   <div class="row">
                       <div class="col-md-4">
                           <p class="card-text text-uppercase"> <strong>Id : </strong></p>
                       </div>
                        <div class="col-md-8">
                            <p class="card-text text-uppercase">@Model.Id</p>
                        </div>
                   </div>
                    <div class="row">
                        <div class="col-md-4">
                            <p class="card-text text-uppercase"> <strong>Name : </strong></p>
                        </div>
                        <div class="col-md-8">
                            <p class="card-text text-uppercase">@Model.Name</p>
                        </div>
                    </div>
                   

               </div>

               
           </div>

       </div>
        <div class="col-md-12">
            <a asp-area="Admin" asp-controller="VehicleType" asp-action="Index" class="btn btn-warning">Back</a>
           
        </div>
   </div>
 </div>
